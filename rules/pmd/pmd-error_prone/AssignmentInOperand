AtomicRule {
    elements:[exp: AtomicRule{}],
    filters: [
        CompositeFilter {
            operator: "OR",
            filters: TemplateFilter: {
                    target: exp,
                    template: AtomicRule {
                    left: VarAssignment{}
                }
            },
            TemplateFilter {
                target: exp,
                template: AtomicRule{
                    right: VarAssignment{}
                }
            }
        }
    ]
}