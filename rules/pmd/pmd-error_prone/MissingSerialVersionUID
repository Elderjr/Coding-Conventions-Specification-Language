AtomicRule:{
    elements:[c as JClass{implements: JInterface{name: "Serializable"}}],
    filters:[{
        TemplateFilter{
            target:c,
            template: JClass{
                fields: [Variable {
                    name: "serialVersionUID",
                    type: PrimitiveType{name: "long"},
                    properties: [
                        Property{
                            name: "public"
                        },
                        Property{
                            name: "static"
                        },
                        Property{
                            final: "final"
                        }
                    ]
                }]
            }
        }
    }]
}