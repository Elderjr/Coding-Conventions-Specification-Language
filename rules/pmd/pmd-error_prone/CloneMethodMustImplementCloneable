AtomicRule {
    elements: [
        c as JClass {
            methods: [m as JMethod {
                name: "clone"
            }]
        }
    ],
    filters: {
        CompositeFilter{
            operator: "AND",
            filters: [
                TemplateFilter{
                    negated: true,
                    target: c,
                    template: JClass {
                        implements: JInterface{
                            name: "Cloneable"
                        }
                    }
                },
                TemplateFilter{
                    negated: true,
                    target: m,
                    template: JMethod {
                        throws: [JClass {
                            name: "CloneNotSupportedException"
                        }],
                        properties: [Property{
                            name: "final"
                        }]
                    }
                }
            ]
        }
    }
}