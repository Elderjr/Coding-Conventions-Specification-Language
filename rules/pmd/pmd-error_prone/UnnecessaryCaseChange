AtomicRule {
    elements: [
        MethodInvocation{
            from: MethodInvocation{elementAccessed: m1 as Method{}},
            elementAccessed: m2 as Method{}
        }
    ],
    filters: [
        CompositeFilter {
            operator: "AND",
            filters:[
                EqualsNamedElementWithLiterals {
                    targets: [m1],
                    names: ["toUpperCase", "toLowerCase"]
                },
                EqualsNamedElementWithLiterals {
                    targets: [m2],
                    names: ["equals", "equalsIgnoreCase"]
                }
            ]
        }
    ]
}