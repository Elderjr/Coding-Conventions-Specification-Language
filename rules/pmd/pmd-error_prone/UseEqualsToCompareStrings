AtomicRule {
    elements: [
        exp as InfixExp{
            operator: "=="
        }
    ],
    filters: [CompositeFilter{
        operator: "OR",
        filters: [
            TemplateFilter{
                target: exp,
                template: InfixExp{
                    left: LiteralValue{type: PrimitiveType{name: "string"}}
                }
            },
            TemplateFilter{
                target: exp,
                template: InfixExp{
                    right: LiteralValue{type: PrimitiveType{name: "string"}}
                }
            }
        ]
    }]
}