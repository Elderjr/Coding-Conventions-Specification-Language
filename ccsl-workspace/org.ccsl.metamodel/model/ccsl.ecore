<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ccsl" nsURI="http://www.example.org/ccsl" nsPrefix="ccsl">
  <eClassifiers xsi:type="ecore:EClass" name="Rule" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="negated" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Boolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RecommendationCompositeRule" eSuperTypes="#//RecommendationRule">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="#//LogicOperator"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rules" lowerBound="2" upperBound="-1"
        eType="#//RecommendationRule" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AtomicRule" abstract="true" eSuperTypes="#//Rule #//Context"/>
  <eClassifiers xsi:type="ecore:EEnum" name="LogicOperator">
    <eLiterals name="AND"/>
    <eLiterals name="OR" value="1"/>
    <eLiterals name="IF_THEN" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RecommendationRule" abstract="true"
      eSuperTypes="#//Rule"/>
  <eClassifiers xsi:type="ecore:EClass" name="FaultTypeDescription">
    <eStructuralFeatures xsi:type="ecore:EReference" name="strategy" lowerBound="1"
        eType="#//faultType/strategy/Strategy" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="actions" lowerBound="1"
        upperBound="-1" eType="#//faultType/action/InjectionAction" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rule" lowerBound="1" eType="#//faultType/FaultTypeRule"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AtomicRecommendationRule" eSuperTypes="#//RecommendationRule #//AtomicRule"/>
  <eClassifiers xsi:type="ecore:EClass" name="Context" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="subject" lowerBound="1"
        eType="#//elements/Element"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="contextElements" upperBound="-1"
        eType="#//elements/Element" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="filters" upperBound="-1"
        eType="#//filters/Filter" containment="true"/>
  </eClassifiers>
  <eSubpackages name="elements" nsURI="http://www.example.org/ccsl/elements" nsPrefix="elements">
    <eClassifiers xsi:type="ecore:EClass" name="CodeElement" eSuperTypes="#//elements/Element">
      <eStructuralFeatures xsi:type="ecore:EReference" name="properties" upperBound="-1"
          eType="#//elements/Property" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="exact" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Boolean"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Property">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Element">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="uniqueName" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"
          iD="true"/>
    </eClassifiers>
    <eSubpackages name="namedElements" nsURI="http://www.example.org/ccsl/elements/namedElements"
        nsPrefix="namedElements">
      <eClassifiers xsi:type="ecore:EClass" name="NamedElement" eSuperTypes="#//elements/CodeElement">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ComplexType" eSuperTypes="#//elements/namedElements/NamedElement #//nonElements/StatementsContainer #//elements/datatype/ObjectType">
        <eStructuralFeatures xsi:type="ecore:EReference" name="fields" upperBound="-1"
            eType="#//elements/namedElements/Variable" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="superTypes" upperBound="-1"
            eType="#//elements/namedElements/ComplexType"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="methods" upperBound="-1"
            eType="#//elements/namedElements/Method" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Variable" eSuperTypes="#//elements/namedElements/NamedElement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//elements/datatype/DataType"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="initialValue" eType="#//elements/statements/Statement"
            containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Method" eSuperTypes="#//elements/namedElements/NamedElement #//nonElements/StatementsContainer">
        <eStructuralFeatures xsi:type="ecore:EReference" name="params" upperBound="-1"
            eType="#//elements/namedElements/Variable" unsettable="true" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="returnType" eType="#//elements/datatype/DataType"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Constructor" eSuperTypes="#//elements/namedElements/Method"/>
      <eClassifiers xsi:type="ecore:EClass" name="Package" eSuperTypes="#//elements/namedElements/NamedElement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="groupedClasses" upperBound="-1"
            eType="#//elements/namedElements/ComplexType" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="VariableVector" eSuperTypes="#//elements/namedElements/Variable">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="dimensions" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"
            unsettable="true"/>
      </eClassifiers>
    </eSubpackages>
    <eSubpackages name="statements" nsURI="http://www.example.org/ccsl/elements/statements"
        nsPrefix="statements">
      <eClassifiers xsi:type="ecore:EClass" name="Statement" eSuperTypes="#//elements/CodeElement"/>
      <eClassifiers xsi:type="ecore:EClass" name="NamedElementAccess" eSuperTypes="#//elements/statements/Statement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="from" eType="#//elements/statements/Statement"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="VariableAccess" eSuperTypes="#//elements/statements/Access"/>
      <eClassifiers xsi:type="ecore:EClass" name="DataTypeAccess" eSuperTypes="#//elements/statements/Access"/>
      <eClassifiers xsi:type="ecore:EClass" name="MethodInvocation" eSuperTypes="#//elements/statements/Access">
        <eStructuralFeatures xsi:type="ecore:EReference" name="args" upperBound="-1"
            eType="#//elements/statements/Statement" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ControlFlow" eSuperTypes="#//elements/statements/Statement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="condition" eType="#//elements/statements/Statement"
            containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Block" eSuperTypes="#//elements/statements/Statement #//nonElements/StatementsContainer"/>
      <eClassifiers xsi:type="ecore:EClass" name="SuperMethodInvocation" eSuperTypes="#//elements/statements/MethodInvocation"/>
      <eClassifiers xsi:type="ecore:EClass" name="VarDeclaration" eSuperTypes="#//elements/statements/Statement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="variable" eType="#//elements/namedElements/Variable"
            containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="InstanceCreation" eSuperTypes="#//elements/statements/Statement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//elements/datatype/ObjectType"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Access" eSuperTypes="#//elements/statements/Statement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="elementAccessed" eType="#//elements/Element"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="from" eType="#//elements/statements/Statement"
            unsettable="true" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ArrayCreation" eSuperTypes="#//elements/statements/Statement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//elements/datatype/ObjectType"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="VarAssignment" eSuperTypes="#//elements/statements/Statement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="variable" eType="#//elements/namedElements/Variable"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="assignment" eType="#//elements/statements/Statement"
            containment="true"/>
      </eClassifiers>
      <eSubpackages name="expressions" nsURI="http://www.example.org/ccsl/elements/statements/expressions"
          nsPrefix="expressions">
        <eClassifiers xsi:type="ecore:EClass" name="OperatorExpression" eSuperTypes="#//elements/statements/Statement">
          <eStructuralFeatures xsi:type="ecore:EReference" name="operands" upperBound="-1"
              eType="#//elements/statements/Statement" containment="true"/>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="ParenthesizedExpression" eSuperTypes="#//elements/statements/Statement">
          <eStructuralFeatures xsi:type="ecore:EReference" name="expression" eType="#//elements/statements/Statement"
              containment="true"/>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="StringConcatenation" eSuperTypes="#//elements/statements/expressions/OperatorExpression"/>
        <eClassifiers xsi:type="ecore:EClass" name="InfixExpression" eSuperTypes="#//elements/statements/expressions/OperatorExpression"/>
        <eClassifiers xsi:type="ecore:EClass" name="Assignment" eSuperTypes="#//elements/statements/expressions/OperatorExpression">
          <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//elements/statements/Statement"
              containment="true"/>
        </eClassifiers>
      </eSubpackages>
      <eSubpackages name="literalValues" nsURI="http://www.example.org/ccsl/elements/statements/literalValues"
          nsPrefix="literalValues">
        <eClassifiers xsi:type="ecore:EClass" name="LiteralValue" eSuperTypes="#//elements/statements/Statement">
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="NullLiteral" eSuperTypes="#//elements/statements/literalValues/LiteralValue"/>
        <eClassifiers xsi:type="ecore:EClass" name="CharacterLiteral" eSuperTypes="#//elements/statements/literalValues/LiteralValue"/>
        <eClassifiers xsi:type="ecore:EClass" name="StringLiteral" eSuperTypes="#//elements/statements/literalValues/LiteralValue"/>
        <eClassifiers xsi:type="ecore:EClass" name="NumberLiteral" eSuperTypes="#//elements/statements/literalValues/LiteralValue"/>
        <eClassifiers xsi:type="ecore:EClass" name="BooleanLiteral" eSuperTypes="#//elements/statements/literalValues/LiteralValue"/>
      </eSubpackages>
      <eSubpackages name="controlFlow" nsURI="http://www.example.org/ccsl/elements/statements/controlFlow"
          nsPrefix="controlFlow">
        <eClassifiers xsi:type="ecore:EClass" name="SwitchStatement" eSuperTypes="#//elements/statements/ControlFlow">
          <eStructuralFeatures xsi:type="ecore:EReference" name="cases" upperBound="-1"
              eType="#//elements/statements/controlFlow/SwitchCaseBlock" containment="true"/>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="SwitchCaseBlock" eSuperTypes="#//elements/statements/Block">
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="default" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Boolean"/>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="IfStatement" eSuperTypes="#//elements/statements/ControlFlow">
          <eStructuralFeatures xsi:type="ecore:EReference" name="thenStatement" eType="#//elements/statements/Statement"
              unsettable="true" containment="true"/>
          <eStructuralFeatures xsi:type="ecore:EReference" name="elseStatement" eType="#//elements/statements/Statement"
              unsettable="true" containment="true"/>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="LoopStatement" eSuperTypes="#//elements/statements/ControlFlow">
          <eStructuralFeatures xsi:type="ecore:EReference" name="body" eType="#//elements/statements/Statement"
              unsettable="true" containment="true"/>
        </eClassifiers>
      </eSubpackages>
      <eSubpackages name="tryCatch" nsURI="http://www.example.org/ccsl/elements/statements/tryCatch"
          nsPrefix="tryCatch">
        <eClassifiers xsi:type="ecore:EClass" name="TryStatement">
          <eStructuralFeatures xsi:type="ecore:EReference" name="catchBlocks" upperBound="-1"
              eType="#//elements/statements/tryCatch/CatchBlock" containment="true"/>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="CatchBlock" eSuperTypes="#//elements/statements/Block">
          <eStructuralFeatures xsi:type="ecore:EReference" name="exception" eType="#//elements/namedElements/Variable"
              containment="true"/>
        </eClassifiers>
      </eSubpackages>
    </eSubpackages>
    <eSubpackages name="datatype" nsURI="http://www.example.org/ccsl/elements/datatype"
        nsPrefix="datatype">
      <eClassifiers xsi:type="ecore:EClass" name="DataType" eSuperTypes="#//elements/Element"/>
      <eClassifiers xsi:type="ecore:EClass" name="PrimitiveType" eSuperTypes="#//elements/datatype/DataType"/>
      <eClassifiers xsi:type="ecore:EClass" name="StringPrimitiveType" eSuperTypes="#//elements/datatype/PrimitiveType"/>
      <eClassifiers xsi:type="ecore:EClass" name="BooleanPrimitiveType" eSuperTypes="#//elements/datatype/PrimitiveType"/>
      <eClassifiers xsi:type="ecore:EClass" name="ShortPrimitiveType" eSuperTypes="#//elements/datatype/PrimitiveType"/>
      <eClassifiers xsi:type="ecore:EClass" name="ObjectType" eSuperTypes="#//elements/datatype/DataType"/>
      <eClassifiers xsi:type="ecore:EClass" name="ParameterizedType" eSuperTypes="#//elements/datatype/ObjectType">
        <eStructuralFeatures xsi:type="ecore:EReference" name="typeParameters" upperBound="-1"
            eType="#//elements/datatype/ObjectType"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//elements/namedElements/ComplexType"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="GenericType" eSuperTypes="#//elements/namedElements/ComplexType"/>
      <eClassifiers xsi:type="ecore:EClass" name="IntPrimitiveType" eSuperTypes="#//elements/datatype/PrimitiveType"/>
      <eClassifiers xsi:type="ecore:EClass" name="ArrayType" eSuperTypes="#//elements/datatype/ObjectType">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="dimensions" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"
            unsettable="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//elements/datatype/DataType"/>
      </eClassifiers>
    </eSubpackages>
  </eSubpackages>
  <eSubpackages name="java" nsURI="http://www.example.org/ccsl/java" nsPrefix="java">
    <eClassifiers xsi:type="ecore:EClass" name="JElement"/>
    <eClassifiers xsi:type="ecore:EClass" name="JavaClass" eSuperTypes="#//elements/namedElements/ComplexType #//java/JElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="nestedClasses" upperBound="-1"
          eType="#//java/JavaClass" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="implements" upperBound="-1"
          eType="#//java/JavaInterface"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="JavaInterface" eSuperTypes="#//java/JElement #//elements/namedElements/ComplexType"/>
    <eClassifiers xsi:type="ecore:EClass" name="JavaMethod" eSuperTypes="#//elements/namedElements/Method">
      <eStructuralFeatures xsi:type="ecore:EReference" name="throws" upperBound="-1"
          eType="#//java/JavaClass"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="JConstructor" eSuperTypes="#//java/JavaMethod"/>
    <eClassifiers xsi:type="ecore:EClass" name="JTryCatch" eSuperTypes="#//java/JElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="try" eType="#//java/JTryClause"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="catches" upperBound="-1"
          eType="#//java/JCatchClause" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="JTryClause" eSuperTypes="#//elements/statements/Block">
      <eStructuralFeatures xsi:type="ecore:EReference" name="resources" upperBound="-1"
          eType="#//elements/statements/Statement" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="JCatchClause" eSuperTypes="#//elements/statements/Block">
      <eStructuralFeatures xsi:type="ecore:EReference" name="exceptions" upperBound="-1"
          eType="#//java/JavaClass"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="param" eType="#//elements/namedElements/Variable"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="InstanceOf" eSuperTypes="#//java/JElement #//elements/statements/Statement #//elements/statements/expressions/OperatorExpression"/>
    <eClassifiers xsi:type="ecore:EClass" name="ThrowStatement" eSuperTypes="#//java/JElement"/>
    <eClassifiers xsi:type="ecore:EClass" name="JFor" eSuperTypes="#//elements/statements/Block #//java/JElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="initializers" upperBound="-1"
          eType="#//elements/statements/Statement" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="updaters" upperBound="-1"
          eType="#//elements/statements/Statement" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="JReturnStatement" eSuperTypes="#//java/JElement #//elements/statements/Statement"/>
    <eClassifiers xsi:type="ecore:EClass" name="GenericType" eSuperTypes="#//elements/namedElements/ComplexType"/>
  </eSubpackages>
  <eSubpackages name="filters" nsURI="http://www.example.org/ccsl/filters" nsPrefix="filters">
    <eClassifiers xsi:type="ecore:EClass" name="Filter" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="negated" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Boolean"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="AtomicFilter" abstract="true" eSuperTypes="#//filters/Filter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="targets" lowerBound="1"
          upperBound="-1" eType="#//elements/Element"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CompositeFilter" eSuperTypes="#//filters/Filter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="filters" lowerBound="1"
          upperBound="-1" eType="#//filters/Filter" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="#//LogicOperator"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PropertyFilter" eSuperTypes="#//filters/AtomicFilter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="properties" lowerBound="1"
          upperBound="-1" eType="#//elements/Property" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TemplateFilter" eSuperTypes="#//filters/AtomicFilter #//Context"/>
    <eClassifiers xsi:type="ecore:EClass" name="DefinesMethodFilter" eSuperTypes="#//filters/AtomicFilter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="methods" lowerBound="1"
          upperBound="-1" eType="#//elements/namedElements/Method" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SameNameFilter" eSuperTypes="#//filters/AtomicFilter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
          eType="#//elements/namedElements/NamedElement"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="ignoreCase" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Boolean"
          defaultValueLiteral="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="RegexLiteralValueFilter" eSuperTypes="#//filters/AtomicFilter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="regex" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CountBlockStatementsFilter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="count" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="EqualsNamedElementWithLiterals" eSuperTypes="#//filters/AtomicFilter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="names" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="ignoreCase" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Boolean"
          defaultValueLiteral="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="RegexMatch" eSuperTypes="#//filters/AtomicFilter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="regex" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypedStatementFilter" eSuperTypes="#//filters/AtomicFilter">
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1"
          eType="#//elements/datatype/DataType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SwitchCaseStatementCount" eSuperTypes="#//filters/AtomicFilter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="min" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"
          unsettable="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="max" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"
          unsettable="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="cases" upperBound="-1"
          eType="#//elements/statements/controlFlow/SwitchCaseBlock" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ClosureFilter" eSuperTypes="#//filters/TemplateFilter">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxDepth" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"
          unsettable="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="minDepth" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"
          unsettable="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="closureElement" lowerBound="1"
          eType="#//elements/Element"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ImplicityContainer" eSuperTypes="#//filters/ClosureFilter"/>
  </eSubpackages>
  <eSubpackages name="nonElements" nsURI="http://www.example.org/ccsl/nonElements"
      nsPrefix="nonElements">
    <eClassifiers xsi:type="ecore:EClass" name="StatementsContainer" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EReference" name="statements" upperBound="-1"
          eType="#//elements/statements/Statement" containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="faultType" nsURI="http://www.example.org/ccsl/faultType" nsPrefix="faultType">
    <eClassifiers xsi:type="ecore:EClass" name="AtomicFaultTypeRule" eSuperTypes="#//AtomicRule #//faultType/FaultTypeRule"/>
    <eClassifiers xsi:type="ecore:EClass" name="FaultTypeRule" abstract="true" eSuperTypes="#//Rule"/>
    <eClassifiers xsi:type="ecore:EClass" name="CompositeFaultTypeRule" eSuperTypes="#//faultType/FaultTypeRule">
      <eStructuralFeatures xsi:type="ecore:EReference" name="rules" lowerBound="2"
          upperBound="-1" eType="#//faultType/FaultTypeRule" containment="true"/>
    </eClassifiers>
    <eSubpackages name="action" nsURI="http://www.example.org/ccsl/faultType/action"
        nsPrefix="action">
      <eClassifiers xsi:type="ecore:EClass" name="InjectionAction" abstract="true">
        <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
            eType="#//elements/Element"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="DeleteAction" eSuperTypes="#//faultType/action/InjectionAction"/>
      <eClassifiers xsi:type="ecore:EClass" name="MoveScopeUpAction" eSuperTypes="#//faultType/action/InjectionAction"/>
      <eClassifiers xsi:type="ecore:EClass" name="DeleteInfixOperator" eSuperTypes="#//faultType/action/InjectionAction"/>
      <eClassifiers xsi:type="ecore:EClass" name="ChangeLiteralValue" eSuperTypes="#//faultType/action/InjectionAction"/>
      <eClassifiers xsi:type="ecore:EClass" name="ReplaceInfixOperator" eSuperTypes="#//faultType/action/InjectionAction">
        <eStructuralFeatures xsi:type="ecore:EReference" name="replacements" lowerBound="1"
            upperBound="-1" eType="#//faultType/action/OperatorReplace" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="OperatorReplace">
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="originalOperator" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="newOperator" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String"/>
      </eClassifiers>
    </eSubpackages>
    <eSubpackages name="strategy" nsURI="http://www.example.org/ccsl/faultType/strategy"
        nsPrefix="strategy">
      <eClassifiers xsi:type="ecore:EClass" name="Strategy" abstract="true"/>
      <eClassifiers xsi:type="ecore:EClass" name="AllStrategy" eSuperTypes="#//faultType/strategy/Strategy"/>
    </eSubpackages>
  </eSubpackages>
</ecore:EPackage>
