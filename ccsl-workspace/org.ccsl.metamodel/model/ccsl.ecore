        <eClassifiers xsi:type="ecore:EClass" name="ForStatement" eSuperTypes="#//elements/statements/controlFlow/LoopStatement #//elements/statements/controlFlow/ConditionalControlFlowStatement">
          <eStructuralFeatures xsi:type="ecore:EReference" name="initializers" upperBound="-1"
