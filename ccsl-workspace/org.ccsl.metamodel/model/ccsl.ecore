            containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="message" eType="#//elements/expressions/Expression"
            containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ExpressionStatement" eSuperTypes="#//elements/statements/Statement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="expression" upperBound="-1"
            eType="#//elements/expressions/Expression" containment="true"/>
      </eClassifiers>
      <eSubpackages name="controlFlow" nsURI="http://www.example.org/ccsl/elements/statements/controlFlow"
