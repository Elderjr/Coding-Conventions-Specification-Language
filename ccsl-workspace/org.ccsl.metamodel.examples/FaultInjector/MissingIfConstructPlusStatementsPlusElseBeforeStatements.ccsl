<?xml version="1.0" encoding="UTF-8"?>
<ccsl:FaultTypeDescription xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:action="http://www.example.org/ccsl/faultType/action" xmlns:ccsl="http://www.example.org/ccsl" xmlns:controlFlow="http://www.example.org/ccsl/elements/statements/controlFlow" xmlns:faultType="http://www.example.org/ccsl/faultType" xmlns:strategy="http://www.example.org/ccsl/faultType/strategy" name="MissingIfConstructPlusStatementsPlusElseBeforeStatements">
  <strategy xsi:type="strategy:AllStrategy"/>
  <actions xsi:type="action:MoveScopeUpAction" target="elseStmt"/>
  <actions xsi:type="action:DeleteAction" target="//@rule/@context/@contextElements.0"/>
  <rule subject="//@rule/@context/@contextElements.0">
    <context><contextElements xsi:type="controlFlow:IfStatement">
      <thenStatement/>
      <elseStatement uniqueName="elseStmt"/>
    </contextElements></context>
  </rule>
</ccsl:FaultTypeDescription>
