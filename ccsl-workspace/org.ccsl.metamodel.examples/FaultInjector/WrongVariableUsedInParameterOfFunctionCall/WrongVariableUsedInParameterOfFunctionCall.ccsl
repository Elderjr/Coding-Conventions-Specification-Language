<?xml version="1.0" encoding="UTF-8"?>
<ccsl:FaultTypeDescription xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:action="http://www.example.org/ccsl/faultType/action" xmlns:ccsl="http://www.example.org/ccsl" xmlns:faultType="http://www.example.org/ccsl/faultType" xmlns:statements="http://www.example.org/ccsl/elements/statements" xmlns:strategy="http://www.example.org/ccsl/faultType/strategy" name="WrongVariableUsedInParameterOfFunctionCall">
  <strategy xsi:type="strategy:AllStrategy"/>
  <actions xsi:type="action:ReplaceVariableAccess" target="//@rule/@contextElements.0/@args.0"/>
  <rule xsi:type="faultType:AtomicFaultTypeRule" subject="//@rule/@contextElements.0/@args.0">
    <contextElements xsi:type="statements:MethodInvocation">
      <args xsi:type="statements:VariableAccess"/>
    </contextElements>
  </rule>
</ccsl:FaultTypeDescription>
