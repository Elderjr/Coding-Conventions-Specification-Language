<?xml version="1.0" encoding="UTF-8"?>
<ccsl:AtomicRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ccsl="http://www.example.org/ccsl" xmlns:statements="http://www.example.org/ccsl/elements/statements" xmlns:tryCatch="http://www.example.org/ccsl/elements/statements/tryCatch" subject="//@context/@contextElements.0">
  <context>
    <contextElements xsi:type="statements:InstanceOf">
      <leftOperand xsi:type="statements:VariableAccess" elementAccessed="//@context/@contextElements.1/@variable"/>
    </contextElements>
    <contextElements xsi:type="tryCatch:CatchClause">
      <variable/>
    </contextElements>
  </context>
</ccsl:AtomicRule>
