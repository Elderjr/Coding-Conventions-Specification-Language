<?xml version="1.0" encoding="UTF-8"?>
<ccsl:AtomicRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:assignment="http://www.example.org/ccsl/elements/statements/assignment" xmlns:ccsl="http://www.example.org/ccsl" xmlns:filters="http://www.example.org/ccsl/filters" xmlns:literalValues="http://www.example.org/ccsl/elements/statements/literalValues" xmlns:statements="http://www.example.org/ccsl/elements/statements" xmlns:variable="http://www.example.org/ccsl/elements/namedElements/variable" subject="//@context/@contextElements.0">
  <context>
    <contextElements xsi:type="assignment:Assignment">
      <rightHandSide xsi:type="literalValues:NullLiteral"/>
    </contextElements>
    <filters xsi:type="filters:TemplateFilter" negated="true" targets="//@context/@contextElements.0" targetTemplate="//@context/@filters.0/@context/@contextElements.0">
      <context>
        <contextElements xsi:type="assignment:Assignment">
          <leftHandSide xsi:type="statements:VariableAccess" elementAccessed="//@context/@filters.0/@context/@contextElements.1"/>
        </contextElements>
        <contextElements xsi:type="variable:FieldVariable" final="true"/>
      </context>
    </filters>
  </context>
</ccsl:AtomicRule>
