<?xml version="1.0" encoding="UTF-8"?>
<ccsl:AtomicRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ccsl="http://www.example.org/ccsl" xmlns:filters="http://www.example.org/ccsl/filters" xmlns:invocation="http://www.example.org/ccsl/elements/statements/invocation" xmlns:method="http://www.example.org/ccsl/elements/namedElements/method" subject="//@context/@contextElements.0">
  <context>
    <contextElements xsi:type="invocation:MethodInvocation" args="" argsKind="EXACT" method="//@context/@contextElements.1"/>
    <contextElements xsi:type="method:Method"/>
    <filters xsi:type="filters:RegexMatch" targets="//@context/@contextElements.1" regex="to(Lower|Upper)Case"/>
  </context>
</ccsl:AtomicRule>
