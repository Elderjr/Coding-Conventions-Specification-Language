<?xml version="1.0" encoding="UTF-8"?>
<ccsl:AtomicRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ccsl="http://www.example.org/ccsl" xmlns:expressions="http://www.example.org/ccsl/elements/statements/expressions" xmlns:java="http://www.example.org/ccsl/java" xmlns:statements="http://www.example.org/ccsl/elements/statements">
  <auxiliaryElements xsi:type="java:JavaClass" name="java.lang.String">
    <methods xsi:type="java:JavaMethod" name="valueOf"/>
  </auxiliaryElements>
  <scope xsi:type="expressions:StringConcatenation">
    <operands xsi:type="statements:MethodInvocation" elementAccessed="//@auxiliaryElements.0/@methods.0"/>
  </scope>
</ccsl:AtomicRule>
