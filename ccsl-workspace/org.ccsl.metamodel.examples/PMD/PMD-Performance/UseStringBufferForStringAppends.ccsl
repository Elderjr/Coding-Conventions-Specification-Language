<?xml version="1.0" encoding="UTF-8"?>
<ccsl:AtomicRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ccsl="http://www.example.org/ccsl" xmlns:expressions="http://www.example.org/ccsl/elements/statements/expressions" xmlns:filters="http://www.example.org/ccsl/filters" xmlns:java="http://www.example.org/ccsl/java">
  <auxiliaryElements xsi:type="java:JavaClass" name="java.lang.String"/>
  <filters xsi:type="filters:TypedStatementFilter" targets="//@scope/@target" type="//@auxiliaryElements.0"/>
  <scope xsi:type="expressions:Assignment" operator="+=">
    <target/>
  </scope>
</ccsl:AtomicRule>
