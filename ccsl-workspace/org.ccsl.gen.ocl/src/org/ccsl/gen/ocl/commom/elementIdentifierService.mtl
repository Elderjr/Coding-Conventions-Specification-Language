[comment encoding = UTF-8 /]
[module elementIdentifierService('http://www.example.org/ccsl')/]

[query public getId(arg0 : Element) : Integer
	= invoke('org.ccsl.gen.ocl.services.ElementIdentifierService', 'getId(ccsl.elements.Element)', Sequence{arg0})
/]

[query public getElementsWithId(anOclAny: OclAny) : Set(Element)
	= invoke('org.ccsl.gen.ocl.services.ElementIdentifierService', 'getElementsWithId()', Sequence{})
/]

[query public isElementRegistered(anOclAny: OclAny, element: Element): Boolean =
  getElementsWithId()->includes(element)
/]

[query public isElementRegistered(element: Element): Boolean =
  getElementsWithId()->includes(element)
/]

[query public clearAllContexts(anOclAny: OclAny) : OclVoid
	= invoke('org.ccsl.gen.ocl.services.ElementIdentifierService', 'clearAllContexts()', Sequence{})
/]

