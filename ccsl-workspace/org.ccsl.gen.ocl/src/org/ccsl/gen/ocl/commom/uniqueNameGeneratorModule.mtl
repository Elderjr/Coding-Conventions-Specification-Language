[comment encoding = UTF-8 /]
[module uniqueNameGeneratorModule('http://www.example.org/ccsl')]
[import org::ccsl::gen::ocl::commom::walkHelperModule /]

[template public getUniqueOclName(element : Element)]
[element.getUniqueName(element.getBaseName()) /]
[/template]

[template public getBaseName(element: Element)]
e
[/template]

[template public getBaseName(element : ComplexType)]
complexType
[/template]

[template public getBaseName(element : JClass)]
clazz
[/template]

[template public getBaseName(element : JInterface)]
