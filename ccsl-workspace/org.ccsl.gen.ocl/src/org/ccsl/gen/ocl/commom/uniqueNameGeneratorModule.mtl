[/template]

[template public getBaseName(element : Variable)]
var
[/template]

[template public getBaseName(element : Method)]
method
[/template]

[template public getBaseName(element : Statement)]
stmt
[/template]

[template public getBaseName(element : MethodInvocation)]
methodInv
[/template]

[template public getBaseName(element : PrimitiveType)]
primitiveType
[/template]

[template public getBaseName(element : LiteralValue)]
literal
[/template]

[template public getBaseName(element : VarDeclaration)]
varDecl
[/template]

[template public getBaseName(element : StringConcatenation)]
strConcat
[/template]

[template public getBaseName(element : Block)]
block
[/template]

[template public getBaseName(element : SwitchCaseBlock)]
