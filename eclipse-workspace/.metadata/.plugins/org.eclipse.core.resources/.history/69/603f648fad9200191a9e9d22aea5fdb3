[module namedElementRegexMatchModule('http://www.example.org/ccsl')]


[template public defineConditionFilter(filter : Filter, element: Element, varName: String) ? (filter.oclIsTypeOf(NamedElementRegexMatch) and element.oclIsKindOf(NamedElement))]
