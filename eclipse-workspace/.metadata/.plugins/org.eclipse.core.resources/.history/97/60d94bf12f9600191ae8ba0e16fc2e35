[comment encoding = UTF-8 /]
[module namedElementRegexMatchModule('http://www.example.org/ccsl') extends org::ccsl::gen::ocl::filters::filterModule /]


[template public generateFilterConditions(filter : NamedElementRegexMatch, element: Element, varName: String) overrides generateFilterConditions]
[let regexFilter: NamedElementRegexMatch = filter.oclAsType(NamedElementRegexMatch)]
[if regexFilter.negated](not [/if][varName /].name.matches('[regexFilter.regex /]')[if regexFilter.negated])[/if][/let]
[/template]

