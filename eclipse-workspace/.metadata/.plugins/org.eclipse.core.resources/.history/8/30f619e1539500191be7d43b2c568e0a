[comment encoding = UTF-8 /]
[module namedElementRegexMatchModule('http://www.example.org/ccsl')]


[template public generateCondition(filter : Filter, element: Element, varName: String) ? (filter.oclIsTypeOf(NamedElementRegexMatch) and element.oclIsKindOf(NamedElement))]
[let regexFilter: NamedElementRegexMatch = filter.oclAsType(NamedElementRegexMatch)]
[if regexFilter.negated](not [/if][varName /].name.matches('[regexFilter.regex /]')[if regexFilter.negated])[/if]
[/let]
[/template]

