package org.ccsl.gen.ocl.services.uniqueNameGeneratorService;


import java.util.HashMap;

import ccsl.elements.Element;

public class UniqueNameGeneratorService {
	private static final Map<Element, Integer> ID_MAP = new HashMap()<>();
	
	public static String generateUniqueName(String baseName, Element element) {
		
		return  baseName + "_" + element.hashCode();
	}
}
