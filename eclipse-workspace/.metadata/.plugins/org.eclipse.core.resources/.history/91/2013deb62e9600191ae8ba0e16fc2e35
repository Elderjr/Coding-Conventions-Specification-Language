[comment encoding = UTF-8 /]
[module propertyFilterModule('http://www.example.org/ccsl')]
[import org::ccsl::gen::ocl::dispatchers::elementDispatcherModule /]

[template public generateFilterConditions(filter : PropertyFilter, element: Element, varName: String) overrides generateFilterConditions]
[if filter.negated]not ([/if][element.callGeneratePropertiesConditions(varName, filter.properties) /][if filter.negated])[/if][/let]
[/template]
